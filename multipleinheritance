interface print
{
	void display();
}
interface copy
{
	void show();
}
class Xerox implements print ,copy
{
	public void display()
	{
		System.out.println("displaying");
	}
	public void show()
	{
		System.out.println("Showing");
	}
}
public class Multipleinheritance {

	public static void main(String[] args) {
		Xerox obj=new Xerox();
		obj.show();
		obj.display();
	}

}
